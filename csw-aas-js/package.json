{
  "name": "csw-aas-js",
  "version": "1.1.0-RC1",
  "description": "This library is a javascript adapter for csw-aas. This is a wrapper over keycloak.js. It provides react components which can be used by UI applications to incorporate authorization and authentication using configured auth server url",
  "author": "TMT Software",
  "license": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tmtsoftware/csw-js.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "jest unit --coverage ",
    "test:it": "jest int --coverage",
    "testAll": "npm run test && npm run test:it",
    "lint": "tsc && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "tsc -w",
    "prepublishOnly": "npm install && npm test"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "keycloak-js": "8.0.1"
  },
  "devDependencies": {
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.3",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-eslint": "^10.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.7.1",
    "pm2": "^3.5.0",
    "prettier": "^1.17.1",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "typescript": "3.2.1"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
